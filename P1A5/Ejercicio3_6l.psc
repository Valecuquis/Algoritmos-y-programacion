	//Ingeniería en Animación Digital
	Definir Y2, Y1, X2, X1 como Enteros;
	Escribir "Introducir el valor de Y1";
	Leer Y1;
	Escribir "Introducir el valor de Y2";
	Leer Y2;
	Escribir "Introducir el valor de X1";
	Leer X1;
	Escribir "Introducir el valor de X2";
	Leer X2;
	Escribir "El resultado de (X2 - X1)^2 + (Y2 - Y1)^2 = ", (X2 - X1)^2 + (Y2 - Y1)^2 ;
