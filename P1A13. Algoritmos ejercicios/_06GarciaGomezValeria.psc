Algoritmo _06GarciaGomezValeria
	// 00594363 Valeria García Gómez
	// Ingeniería en Animación Digital
	// Sumar diez números introducidos por teclado.
	
	Definir suma, num como Enteros;
	
	Para n <- 1 Hasta 10
		Escribir "Ingrese un número"
		Leer num;
		suma <- suma + num;
	FinPara
	
	Escribir "La suma es: ", suma;
	
	
FinAlgoritmo
